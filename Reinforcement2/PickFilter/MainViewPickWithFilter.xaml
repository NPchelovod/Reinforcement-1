<Window x:Class="Reinforcement.PickFilter.MainViewPickWithFilter"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Reinforcement.PickFilter"
             mc:Ignorable="d" Height="130" Width="300" WindowStartupLocation="CenterScreen">
    <Window.Effect>
        <DropShadowEffect/>
    </Window.Effect>
    <StackPanel Margin="0,0,0,7">
        <StackPanel Orientation="Horizontal" HorizontalAlignment="Center" Width="226">
            <TextBlock Margin="5" Text="Тип элемента: "/>
            <TextBox  x:Name="textName" Margin="5" Width="128" />
        </StackPanel>
        <StackPanel Orientation="Horizontal" HorizontalAlignment="Center">
            <Button Click="Click_Select" Margin="5" Padding="5" Content="Выбрать" Width="80" />
            <Button Click="Click_Cancel" Margin="5" Padding="5" Content="Отмена" Width="80" />
        </StackPanel>
    </StackPanel>
</Window>
